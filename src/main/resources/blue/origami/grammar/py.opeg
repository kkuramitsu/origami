/*
  Python 3 Grammar
  Based on grammar from Python Language Reference[1] and Python3.g4[2]
  [1] http://docs.python.jp/3.3/reference/index.html
  [2] https://github.com/antlr/grammars-v4/blob/master/python3/Python3.g4
  
  Author:
    Shun Honda (https://github.com/shun-honda)
*/

/* Starting Point */

File = <on !InParentheses { ( NEWLINE / EOS / $(NonIndentStatement) / $(Statement) )* #Source } > EOF

/* Code Layout */

Indent  = S*
NEWLINE = 
	'\r'? '\n'
	/ '\r'
S       = [ \t]
_       = S*
__      = ( S / '\\' NEWLINE / COMMENT / <if InParentheses> NEWLINE )*
COMMENT = _ '#' ( !'\n' . )*
EOS     = 
	';'
	/ NEWLINE
	/ COMMENT
EOF     = !.

// ====================
// Def
// ====================

example FuncDef '''
def sum_and_stringify(nums: List[int]) -> str:  
    """Adds up the numbers in a list and returns the result as a string."""
    return str(sum(nums))
'''

FuncDef = {
	"def" __ 
	$name(Name) __ 
	'(' <on InParentheses ( __ $(ParameterList)? __ )> ')' 
	( _ '->' _ $type(Test) )?  __ 
	':' __ $body(Suite) 
	#FuncDecl 
}

ParameterList = 
	{ '**' _ $(Parameter) #Params }
	/ { '*' ( _ $(Parameter) )? ( __ ',' __ $(Parameter) )* ( __ ',' __ '**' _ $(Parameter) )? #Params }
	/ { ( $(DefParameter) ( __ ',' __ $(DefParameter) )* )? ( __ ',' __ ('**' _ $(Parameter) / '*' ( _ $(Parameter) )? ( __ ',' __ $(Parameter) )* ( __ ',' __ '**' _ $(Parameter) )?) )? ( __ ',' )? #Params }

DefParameter = Parameter 
	
Parameter = {
	$name(Name) 
	(__ ':' __ $type(Test))? 
	(__ '=' __ $expr(Test))?
	#DyParam 
}
	
VarArgList
	= { '**' _ $(VarArg) #VarArgs }
	/ { '*' ( _ $(VarArg) )? ( __ ',' __ $(VarArg) )* ( __ ',' __ '**' _ $(VarArg) )? #VarArgs }
	/ { ( $(DefVarArg) ( __ ',' __ $(DefVarArg) )* )? ( __ ',' __ ('**' _ $(VarArg) / '*' ( _ $(VarArg) )? ( __ ',' __ $(VarArg) )* ( __ ',' __ '**' _ $(VarArg) )?) )? ( __ ',' )? #VarArgs }

DefVarArg = 
	VarArg 
	{$name __ '=' __ $expr(Test) #Initialize }?

VarArg = Name

// ====================
//      Decorator
// ====================

Decorated = { 
	$(Decorators) _ 
	($(ClassDef) / $(FuncDef)) 
	#Decorated 
}

Decorators = 
	Decorator 
	{$ ( __ $(Decorator) )+ #List }?

Decorator = {
	'@' 
	$(DottedName) 
	( __ '(' <on InParentheses ( __ $(ArgList)? __ )> ')' )? 
	_ NEWLINE 
	#Decorator 
}

// ====================
//      Statement
// ====================

Statement = 
	SimpleStatement
	/ CompoundStatement

SimpleStatement = 
	SmallStatement 
	{$ ( _ ';' __ $(SmallStatement) )+ #List }? _ ';'? ( '#' ( !'\n' . )* )? (NEWLINE / !.)
	
SmallStatement = 
	ExprStatement
	/ DelStatement
	/ PassStatement
	/ FlowStatement
	/ ImportStatement
	/ GlobalStatement
	/ NonLocalStatement
	/ AssertStatement

DelStatement = {
	"del" __ 
	$expr(TestList) 
	#Delete 
}

PassStatement = {
	"pass" 
	#EmptyStmt
}

FlowStatement 
	= BreakStatement
	/ ContinueStatement
	/ ReturnStatement
	/ RaiseStatement
	/ YieldStatement
	
BreakStatement = {
	"break" 
	#BreakStmt
}

ContinueStatement = {
	"continue" 
	#ContinueStmt 
}

ReturnStatement = {
	"return" 
	( __ $expr(TestList) )? 
	#ReturnStmt 
}

RaiseStatement = {
	"raise" ( __ $expr(TestList) ( __ $({ "from" __ $expr(Test) #From }) )? )? #Raise 
}

YieldStatement = YieldExpr
ImportStatement = 
	ImportName
	/ ImportFrom

ImportName = { 
	"import" __ 
	$name(DottedAsNames) 
	#Import 
}

ImportFrom = {
	$({ "from" __ ('.'* $(Module) / '.'+) #From }) __ 
	$({ "import" __ ('(' <on InParentheses ( __ $(DottedAsNames) ( _ ',' )? __ )> ')' / $(DottedAsNames) / '*') #Import }) 
	#FromImport 
}

// { @{"from" __ @Module #From } __ @{"import" __ '*' #Import } #FromImport }

Module  = { 
	( $(Name) '.' )* 
	$(Name) 
	#Field 
}

/*ImportList = ImportAsName {$ ( __ ',' __ $(ImportAsName) )+ #List }?
ImportAsName                        = Name {$ __ "as" __ $name(Name) #As }?*/

DottedAsNames = 
	DottedAsName 
	{$ ( __ ',' __ $(DottedAsName) )+ #List }?

DottedAsName = 
	DottedName 
	{$ __ "as" __ $name(Name) #As }?
	
DottedName = 
	Name 
	{$ ( __ '.' __ $(Name) )+ #Field }?
	
GlobalStatement = {
	"global" __ $label(NameList) 
	#PyGlobalStmt 
}

NonLocalStatement = {
	"nonlocal" __ 
	$label(NameList) 
	#PyNonLocalStmt 
}

NameList = 
	Name 
	{$ ( __ ',' __ $(Name) )+ #List }?
	
AssertStatement = {
	 "assert" __ 
	 $cond(Test)
	 (__ ',' __ $msg(Test))?
	 #AssertStmt 
}

CompoundStatement 
	= IfStatement
	/ WhileStatement
	/ ForStatement
	/ TryStatement
	/ WithStatement
	/ FuncDef
	/ ClassDef
	/ Decorated

IfStatement = { 
	"if" __ 
	$cond(Test) __ 
	':' __ $then(Suite) 
	( <match Indent> $elif(Elif) )* 
	( <match Indent> $else(Else) )? 
	#IfStmt 
}

Elif = { 
	"elif" __ 
	$cond(Test) __ 
	':' __ $then(Suite) 
	#Elif 
}

Else = {
	"else" __ 
	':' __ 
	$then(Suite) 
	#Else 
}

WhileStatement = {
	"while" __ 
	$cond(Test) __ 
	':' __ $body(Suite) 
	( <match Indent> $else(Else) )? 
	#While 
}

ForStatement = {
	"for" __ 
	$name(ExprList) __ 
	"in" __ $iter(TestList) __ 
	':' __ $body(Suite) 
	( <match Indent> $(Else) )? 
	#ForEachStmt 
}

TryStatement = {
	"try" __ 
	':' __ $try(Suite) 
	(
		( <match Indent> $(Except) )+ 
		( <match Indent> $(Else) )? 
		( <match Indent> $(Finally) )? 
		/ <match Indent> $(Finally)
	) 
	#Try 
}

Except = { 
	"except" 
	( __ $expr(TestList) 
	( __ $({ "as" __ $name(Name) #As }) )? )? __ 
	':' __ $body(Suite) 
	#Except 
}

Finally = {
	"finally" __ 
	':' __ $finally(Suite) 
	#Finally 
}

WithStatement = {
	"with" __ 
	$(WithItemList) __ 
	':' __ $body(Suite) 
	#With 
}

WithItemList = 
	WithItem 
	{$ ( __ ',' __ $(WithItem) )+ #List }?

WithItem = 
	Test 
	{$expr __ "as" __ $name(Expr) #As }?

Suite = 
	{ $(SimpleStatement) #Block }
	/ NEWLINE ( _ EOS )* 
	{ <block ( <symbol Indent> $(Statement) ( <match Indent> $(Statement) / _ EOS )* )> #Block }

NonIndentStatement = 
	{ "if" __ $cond(Test) __ ':' __ $then(Suite) $(Elif)* $(Else)? #If }
	/ { "while" __ $cond(Test) __ ':' __ $body(Suite) $(Else)? #While }
	/ { "for" __ $name(ExprList) __ "in" __ $iter(TestList) __ ':' __ $body(Suite) $(Else)? #For }
	/ { "try" __ ':' __ $try(Suite) ( $(Except)+ $(Else)? $(Finally)? / $(Finally)) #Try }

// ====================
//      Expression
// ====================

ExprStatement = 
	TestListStarExpr {$left __ ASSIGN __ $right(YieldExpr/TestList) }
	/ TestListStarExpr {$left __ '=' __ $right(YieldExpr/TestListStarExpr) #AssignStmt }*

ASSIGN = 
	'+=' #AddAssign
	/ '-=' #SubAssign
	/ '*=' #MulAssign
	/ '/=' #DivAssign
	/ '%=' #ModAssign
	/ '&=' #AndAssign
	/ '|=' #OrAssign
	/ '^=' #XorAssign
	/ '<<=' #LShiftAssign
	/ '>>=' #RShiftAssign
	/ '**=' #PowerAssign
	/ '//=' #FloorDivAssign
	
TestListStarExpr = 
	TestStarExpr 
	{$ (__ ',' __ $(TestStarExpr))+ #MultiExpr }? ( __ ',' )?

TestStarExpr = 
	Test
	/ StarExpr

TestList = 
	Test 
	{$ (__ ',' __ $(Test))+ #MultiExpr }? ( __ ',' )?

Test = 
	OrTest {$then __ "if" __ $cond(OrTest) __ "else" __ $else(Test) #IfExpr }?
	/ LambDef

TestNoCond 
	= OrTest
	/ LambDefNoCond

LambDef = {
	"lambda" __ 
	$param(VarArgList)? __ 
	':' __ $body(Test) 
	#Lambda 
}

LambDefNoCond = {
	"lambda" __ 
	$param(VarArgList)? __ 
	':' __ $body(TestNoCond) 
	#Lambda 
}

OrTest = 
	AndTest 
	{$left __ "or" __ $right(AndTest) #OrExpr }*
	
AndTest = 
	NotTest 
	{$left __ "and" __ $right(NotTest) #AndExpr }*

NotTest = 
	{ "not" __ $expr(NotTest) #NotExpr }
	/ Comparison
	
Comparison = 
	StarExpr 
	{$left __ CMPR __ $right(StarExpr) }*

CMPR = 
	'>=' #GteExpr
	/ '<=' #LteExpr
	/ '<' !'<' #LtExpr
	/ '>' !'>' #GtExpr
	/ '==' #EqExpr
	/ '!=' #NeExpr
	/ "in" #InExpr
	/ "not" __ "in" #NotInExpr
	/ "is" __ "not" #IsNotExpr
	/ "is" #IsExpr

StarExpr = '*'? Expr

Expr = 
	XOrExpr 
	{$left __ '|' !'|' __ $right(XOrExpr) #BitwiseOrExpr }*
	
XOrExpr = 
	AndExpr 
	{$left __ '^' __ $right(AndExpr) #BitwiseXorExpr }*
	
AndExpr = 
	ShiftExpr 
	{$left __ '&' !'&' __ $right(ShiftExpr) #BitwiseAndExpr }*
	
ShiftExpr = 
	ArithExpr 
	{$left __ ('<<' #LShiftExpr / '>>' #RShiftExpr) __ $right(ArithExpr) }*
	
ArithExpr = 
	Term 
	{$left __ ('+' #AddExpr / '-' #SubExpr) __ $right(Term) }*

Term = 
	Factor 
	{$left __ ('*' #MulExpr / '%' #ModExpr / '//' #FloorDivExpr / '/' #DivExpr) __ $right(Factor) }*

Factor = 
	{ ('+' #PlusExpr / '-' #MinusExpr / '~' #ComplExpr) __ $expr(Factor) }
	/ Power
	
Power = 
	AtomTrailer 
	{$left __ '**' __ $right(Factor) #PowerExpr }?
	
AtomTrailer = 
	Atom 
	{$recv __ addTrailer }*

Atom  = 
	'(' <on InParentheses ( __ { ( $(YieldExpr) / $(TestListComp) )? #Atom } __ )> ')'
	/ '[' <on InParentheses ( __ { $(TestListComp)? #Atom } __ )> ']'
	/ '{' <on InParentheses ( __ { $(DictorsetMaker)? #Atom } __ )> '}'
	/ '...'
	/ NoParenthesesArgumentList_FuncCall
	/ Name
	/ Number
	/ String {$ ( __ $(String) )+ #MultiStringExpr }?
	/ None
	/ True
	/ False

TestListComp = { 
	$(Test) __ 
	($(CompFor) / ( ',' __ $(Test) __ )* __ ','?) 
	#List 
}

addTrailer 
	= '(' <on InParentheses ( __ $param(ArgList)? __ )> ')' #ApplyExpr
	/ '[' <on InParentheses ( __ $(SubScriptList) __ )> ']' #SliceExpr
	/ '.' __ $name(Name) #Getter

SubScriptList = 
	SubScript 
	{$ ( __ ',' __ $(SubScript) )+ #List }? __ ','?
	
SubScript = 
	{ $(Test)? __ ':' __ $(Test)? __ addSliceOp? #KeyValue }
	/ Test

addSliceOp = ':' $(Test)?

ExprList = { 
	$(StarExpr) 
	( __ ',' __ $(StarExpr) )* __ ','? 
	#List 
}

DictorsetMaker = { 
	$(Test) __ 
	(':' __ $(Test) __ ($(CompFor) / ( ',' __ $(Test) __ ':' __ $(Test) __ )* ','?) / $(CompFor) / ( ',' __ $(Test) __ )* ','?) 
	#Set 
}

ClassDef = { 
	"class" __ 
	$name(Name) __ 
	( '(' <on InParentheses ( __ $super(ArgList) __ )> ')' __ )? 
	':' __ $body(Suite) 
	#ClassDecl 
}

ArgList = 
	{ '**' _ $(Test) #Args }
	/ { '*' ( _ $(Test) )? ( __ ',' __ Argument )* ( __ ',' __ '**' _ $(Test) )? #Args }
	/ { ( Argument ( __ ',' __ Argument )* )? ( __ ',' __ ('**' _ $(Test) / '*' ( _ $(Test) )? ( __ ',' __ Argument )* ( __ ',' __ '**' _ $(Test) )?) )? ( __ ',' )? #Args }

Argument = 
	$({ $(Test) ( __ $(CompFor) )? __ '=' __ $(Test) #Initialize })
	/ $(Test) ( __ $(CompFor) )?

CompIter
	= CompFor
	/ CompIf

CompFor = { 
	"for" __ 
	$name(ExprList) __ 
	"in" __ 
	$iter(OrTest) !( _ ',' ) !( _ ':' ) 
	( __ $body(CompIter) )? 
	#For 
}

CompIf = { 
	"if" __ $(TestNoCond) 
	( __ $(CompIter) )? 
	#if 
}

YieldExpr = {
	"yield" __ 
	$expr(YieldArg)? 
	#YieldExpr 
}

YieldArg = 
	{ "from" __ $(Test) #From }
	/ TestList

NoParenthesesArgumentList_FuncCall = 
	Name 
	{$ _ !( KEYWORD S+ / '(' / ',' ) Argument #Apply }

None = { "None" #NullExpr }
True = { "True" #TrueExpr }
False = { "False" #FalseExpr }
Name = { NAME #NameExpr }

NAME = 
	!( KEYWORD (S+ / NEWLINE / ':') ) 
	ID_START ID_CONTINUE*

KEYWORD = 
	'False'
	/ 'class'
	/ 'finally'
	/ 'is'
	/ 'return'
	/ 'None'
	/ 'continue'
	/ 'for'
	/ 'lambda'
	/ 'try'
	/ 'True'
	/ 'def'
	/ 'from'
	/ 'nonlocal'
	/ 'while'
	/ 'and'
	/ 'del'
	/ 'global'
	/ 'not'
	/ 'with'
	/ 'assert'
	/ 'as'
	/ 'elif'
	/ 'if'
	/ 'or'
	/ 'yield'
	/ 'else'
	/ 'import'
	/ 'pass'
	/ 'break'
	/ 'except'
	/ 'in'
	/ 'raise'
	
"False"                             = 'False' !ID_CONTINUE
"class"                             = 'class' !ID_CONTINUE
"finally"                           = 'finally' !ID_CONTINUE
"is"                                = 'is' !ID_CONTINUE
"return"                            = 'return' !ID_CONTINUE
"None"                              = 'None' !ID_CONTINUE
"continue"                          = 'continue' !ID_CONTINUE
"for"                               = 'for' !ID_CONTINUE
"lambda"                            = 'lambda' !ID_CONTINUE
"try"                               = 'try' !ID_CONTINUE
"True"                              = 'True' !ID_CONTINUE
"def"                               = 'def' !ID_CONTINUE
"from"                              = 'from' !ID_CONTINUE
"nonlocal"                          = 'nonlocal' !ID_CONTINUE
"while"                             = 'while' !ID_CONTINUE
"and"                               = 'and' !ID_CONTINUE
"del"                               = 'del' !ID_CONTINUE
"global"                            = 'global' !ID_CONTINUE
"not"                               = 'not' !ID_CONTINUE
"with"                              = 'with' !ID_CONTINUE
"assert"                            = 'assert' !ID_CONTINUE
"as"                                = 'as' !ID_CONTINUE
"elif"                              = 'elif' !ID_CONTINUE
"if"                                = 'if' !ID_CONTINUE
"or"                                = 'or' !ID_CONTINUE
"yield"                             = 'yield' !ID_CONTINUE
"else"                              = 'else' !ID_CONTINUE
"import"                            = 'import' !ID_CONTINUE
"pass"                              = 'pass' !ID_CONTINUE
"break"                             = 'break' !ID_CONTINUE
"except"                            = 'except' !ID_CONTINUE
"in"                                = 'in' !ID_CONTINUE
"raise"                             = 'raise' !ID_CONTINUE

Number = 
	ImagNumber
	/ FloatNumber
	/ Integer

Integer = {
	(OCT_INTEGER / HEX_INTEGER / BIN_INTEGER / DECIMAL_INTEGER)
	 #IntExpr
} [lL]? 

DECIMAL_INTEGER = 
	NON_ZERO_DIGIT DIGIT*
	/ '0'+

OCT_INTEGER = '0' [oO] OCT_DIGIT+
HEX_INTEGER = '0' [xX] HEX_DIGIT+
BIN_INTEGER = '0' [bB] BIN_DIGIT+

FloatNumber    = { (EXPONENT_FLOAT / POINT_FLOAT) #DoubleExpr }
ImagNumber     = { ($(FloatNumber) / INT_PART) [jJ] #ImaginaryExpr }

NON_ZERO_DIGIT = [1-9]
DIGIT          = [0-9]
OCT_DIGIT      = [0-7]
HEX_DIGIT      = [0-9a-fA-F]
BIN_DIGIT      = [01]
POINT_FLOAT    = 
	INT_PART? FRACTION
	/ INT_PART '.'
EXPONENT_FLOAT = (POINT_FLOAT / INT_PART) EXPONENT
INT_PART       = DIGIT+
FRACTION       = '.' DIGIT+
EXPONENT       = [eE] [+-]? DIGIT+



String = 
	StringLiteral
	/ BytesLiteral

StringLiteral = [uU]? [rR]? (ShortString / LongString)
BytesLiteral  = [bB] [rR]? (ShortBytes / LongBytes)
ShortString   = 
	!'\'\'\'' '\'' { ( STRING_ESCAPE_SEQ / ![\\\r\n'] . )* #StringExpr } '\''
	/ !'"""' '"' { ( STRING_ESCAPE_SEQ / ![\\\r\n"] . )* #StringExpr } '"'
LongString = 
	'\'\'\'' { ( STRING_ESCAPE_SEQ / ![\\'] . / !'\'\'\'' '\'' )* #String } '\'\'\''
	/ '"""' { ( STRING_ESCAPE_SEQ / ![\\"] . / !'"""' '"' )* #String } '"""'

STRING_ESCAPE_SEQ = 
	'\\' .

ShortBytes
	= !'\'\'\'' '\'' { ( SHORT_BYTES_CHAR_NO_SINGLE_QUOTE / BYTES_ESCAPE_SEQ )* #Bytes } '\''
	/ !'"""' '"' { ( SHORT_BYTES_CHAR_NO_DOUBLE_QUOTE / BYTES_ESCAPE_SEQ )* #Bytes } '"'
	
LongBytes = 
	'\'\'\'' { (!'\'\'\'' LONG_BYTES_ITEM)* #Bytes } '\'\'\''
	/ '"""' { (!'"""' LONG_BYTES_ITEM)* #Bytes } '"""'

LONG_BYTES_ITEM = 
	LONG_BYTES_CHAR
	/ BYTES_ESCAPE_SEQ

SHORT_BYTES_CHAR_NO_SINGLE_QUOTE    = [\u0000-\u0009\u000B-\u000C\u000E-\u0026\u0028-\u005B\u005D-\u007F]
SHORT_BYTES_CHAR_NO_DOUBLE_QUOTE    = [\u0000-\u0009\u000B-\u000C\u000E-\u0021\u0023-\u005B\u005D-\u007F]
LONG_BYTES_CHAR                     = [\u0000-\u005B\u005D-\u007F]
